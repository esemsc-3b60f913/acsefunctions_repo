    "import numpy as np\n",
    "from acsefunctions import exp, sinh, gamma, bessel\n",
    "\n",
    "x = np.linspace(-2, 2, 100)\n",
    "print(exp(x))       # Exponential\n",
    "print(sinh(x))      # Hyperbolic sine\n",
    "print(gamma(0.5))   # ~sqrt(pi)\n",
    "print(bessel(0, x)) # Bessel J_0(x)"
